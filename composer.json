{
    "name": "symfonycasts/messenger-monitor-bundle",
    "description": "Symfony bundle for information about your Messenger queues and transports",
    "type": "symfony-bundle",
    "require": {
        "php": "^8.0",
        "ext-json": "*",
        "symfony/config": "^5.4|^6.0",
        "symfony/dependency-injection": "^5.4|^6.0",
        "symfony/event-dispatcher": "^5.4|^6.0",
        "symfony/framework-bundle": "^5.4|^6.0",
        "symfony/http-foundation": "^5.4|^6.0",
        "symfony/http-kernel": "^5.4|^6.0",
        "symfony/messenger": "^5.4|^6.0",
        "symfony/polyfill-uuid": "^1.23",
        "symfony/routing": "^5.4|^6.0",
        "symfony/twig-bundle": "^5.4|^6.0",
        "twig/twig": "^2.14.7|^3.0"
    },
    "require-dev": {
        "doctrine/dbal": "^2.13.3",
        "doctrine/doctrine-bundle": "^2.0",
        "doctrine/orm": "^2.10",
        "php-cs-fixer/shim": "^3.17",
        "symfony/browser-kit": "^5.4|^6.0",
        "symfony/css-selector": "^5.4|^6.0",
        "symfony/doctrine-messenger": "^6.0",
        "symfony/phpunit-bridge": "^5.4|^6.0",
        "symfony/security-bundle": "^5.4|^6.0",
        "vimeo/psalm": "^4.0"
    },
    "conflict": {
        "symfony/doctrine-bridge": "<5.4"
    },
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "SymfonyCasts\\MessengerMonitorBundle\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "SymfonyCasts\\MessengerMonitorBundle\\Tests\\": "tests/"
        }
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true
    },
    "authors": [
        {
            "name": "Peter Dietrich",
            "email": "xosofox@gmail.com"
        },
        {
            "name": "Nicolas PHILIPPE",
            "email": "nikophil@gmail.com"
        }
    ]
}
